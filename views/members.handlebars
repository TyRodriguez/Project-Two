<head>
  <title>Passport Authentication</title>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link href="stylesheets/style.css" rel="stylesheet">
</head>

<body>
  <nav class="navbar navbar-default">
    <div class="container-fluid">
      <div class="navbar-header">
        <a class="navbar-brand" href="/logout">
          Logout
        </a>
      </div>
    </div>
  </nav>
  <div class="container">
    <div class="row">
      <div class="col-md-6 col-md-offset-3">
        {{!-- need handlebars here to import member name from login/signup? --}}
        <h2>Welcome <span class="member-name"></span></h2>
      </div>
    </div>
    <form class="restaurant">
      <div class="form-group">
        <label for="exampleRestaurantName">Restaurant Name</label>
        <input type="text" class="form-control" id="restaurantName" placeholder="Restaurant Name">
      </div>
      <div class="form-group">
        <label for="examplePhoneNumber">Phone Number</label>
        <input type="number" class="form-control" id="phoneNumber" placeholder="Phone Number (no spaces)">
      </div>
      <div class="form-group">
        <label for="exampleAddress">Address</label>
        <input type="text" class="form-control" id="address" placeholder="Full Address">
      </div>
      <div class="form-group">
        <label for="exampleHours">Hours</label>
        <input type="text" class="form-control" id="hours" placeholder="Daily Hours">
      </div>
      <button class="button brickRedButton" id="tequilabtn">Next</button>
    </form>
  </div>


  <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
  <script type="text/javascript" src="./js/members.js"></script>

</body>

</html>